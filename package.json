{
  "name": "@superfleb/draggable",
  "version": "0.1.3",
  "description": "Draggable elements for Vue, React, and JS",
  "type": "module",
  "license": "UNLICENSED",
  "author": {
    "name": "FLEB (a.k.a. SuperFLEB)",
    "url": "https://github.com/SuperFLEB"
  },
  "files": [
    "/dist/**/*",
    "/src/**/*",
    "/README.md"
  ],
  "packageManager": "yarn@4.5.2",
  "scripts": {
    "clean": "rimraf dist",
    "build": "run clean && rollup --config",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions .ts,.tsx,.js,.jsx,.vue src/"
  },
  "types": "./dist/",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      },
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      }
    },
    "./core": {
      "require": {
        "default": "./dist/core/index.cjs",
        "types": "./dist/core/index.d.ts"
      },
      "import": {
        "default": "./dist/core/index.mjs",
        "types": "./dist/core/index.d.ts"
      }
    },
    "./vue": {
      "require": {
        "default": "./dist/vue/index.cjs",
        "types": "./dist/vue/index.d.ts"
      },
      "import": {
        "default": "./dist/vue/index.mjs",
        "types": "./dist/vue/index.d.ts"
      }
    },
    "./react": {
      "require": {
        "default": "./dist/react/index.cjs",
        "types": "./dist/react/index.d.ts"
      },
      "import": {
        "default": "./dist/react/index.mjs",
        "types": "./dist/react/index.d.ts"
      }
    },
    "./js": {
      "require": {
        "default": "./dist/js/index.cjs",
        "types": "./dist/js/index.d.ts"
      },
      "import": {
        "default": "./dist/js/index.mjs",
        "types": "./dist/js/index.d.ts"
      }
    }
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/react": "^19.0.10",
    "@types/vue": "^2.0.0",
    "madge": "^8.0.0",
    "react": "16.8.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.9",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "tslint": "^6.1.3",
    "typescript": "^5.8.2",
    "vue": "3.0.0"
  }
}